=form_tag projects_path, {method: :get, class: :well} do
  .row
    .col-md-12
      .form-group.form-group-lg
        =text_field_tag :q, params[:q], class: 'form-control search-input', placeholder: 'Search Projects'
  .row
    .col-md-12
      .form-group
        =submit_tag 'Search', class: 'btn btn-primary'
