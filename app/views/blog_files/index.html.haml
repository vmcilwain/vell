#container
  .page-header
    %header
      %h1 Article Files
    =link_to 'Articles Listing', blogs_path

  .row
    .col-md-12
      =paginate @blog_files
      =render 'search'
  %table.table.table-striped
    %thead
      %tr
        %th File name
        %th description
        %th Created On
    %tbody
    =render partial: 'admin_blog_file', collection: @blog_files, as: :blog_file
