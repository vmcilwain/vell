.page-header
  %header
    %h1 Article Files
  =link_to 'Blog Listing', blogs_path
.row
  .col-md-12
    =paginate @blog_files
    =render 'search'
%table.table-striped
  %thead
    %tr
      %th File Name
      %th Description
      %th Blog &nbsp;
      %th Created
  %tbody
    =render @blog_files
